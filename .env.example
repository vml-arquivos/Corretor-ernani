# ============================================
# CONFIGURAÇÃO DO BANCO DE DADOS
# ============================================

# OPÇÃO 1: Supabase (Recomendado para início)
# Obtenha sua connection string em: https://supabase.com/dashboard/project/_/settings/database
DATABASE_URL=postgresql://postgres:[SUA-SENHA]@db.[SEU-PROJETO].supabase.co:5432/postgres

# OPÇÃO 2: PostgreSQL Local (Descomente para usar PostgreSQL local)
# DATABASE_URL=postgresql://corretor:corretorpassword@db:5432/corretordasmansoes
# POSTGRES_DB=corretordasmansoes
# POSTGRES_USER=corretor
# POSTGRES_PASSWORD=corretorpassword
# POSTGRES_PORT=5432

# ============================================
# AUTENTICAÇÃO E SEGURANÇA
# ============================================

# JWT Secret (OBRIGATÓRIO - Gere com: openssl rand -base64 32)
JWT_SECRET=GERAR_COM_OPENSSL_RAND_BASE64_32

# ============================================
# MANUS OAUTH (OBRIGATÓRIO)
# ============================================
# Obtenha suas credenciais em: https://portal.manus.im

VITE_APP_ID=seu_app_id_manus_aqui
OWNER_OPEN_ID=seu_owner_open_id_aqui
OWNER_NAME=Ernani Nunes

# URLs do OAuth (Padrão - não alterar)
OAUTH_SERVER_URL=https://oauth.manus.im
VITE_OAUTH_PORTAL_URL=https://portal.manus.im

# ============================================
# MANUS FORGE API (IA) - OBRIGATÓRIO
# ============================================
# Obtenha suas chaves em: https://portal.manus.im

# Backend API Key
BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=sua_chave_api_forge_backend_aqui

# Frontend API Key
VITE_FRONTEND_FORGE_API_URL=https://forge.manus.im
VITE_FRONTEND_FORGE_API_KEY=sua_chave_api_forge_frontend_aqui

# ============================================
# N8N INTEGRATION (OPCIONAL)
# ============================================
# Configure apenas se você tiver um servidor N8N

N8N_WEBHOOK_URL=https://seu-n8n.com/webhook
N8N_API_KEY=sua_chave_n8n_aqui

# ============================================
# AWS S3 (OPCIONAL)
# ============================================
# Configure para upload de imagens

AWS_ACCESS_KEY_ID=sua_chave_aws_aqui
AWS_SECRET_ACCESS_KEY=seu_secret_aws_aqui
AWS_S3_BUCKET=seu-bucket-s3-aqui
AWS_REGION=us-east-1

# ============================================
# ANALYTICS (OPCIONAL)
# ============================================

VITE_ANALYTICS_ENDPOINT=https://seu-analytics.com
VITE_ANALYTICS_WEBSITE_ID=seu_id_analytics_aqui

# ============================================
# CONFIGURAÇÕES DA APLICAÇÃO
# ============================================

# Porta da aplicação
APP_PORT=3000

# Informações do App
VITE_APP_TITLE=Corretor das Mansões - Ernani Nunes
VITE_APP_LOGO=https://seu-logo.com/logo.png

# ============================================
# PGADMIN (OPCIONAL - Apenas para desenvolvimento)
# ============================================

PGADMIN_PORT=8080
PGADMIN_EMAIL=admin@corretor.com
PGADMIN_PASSWORD=admin
