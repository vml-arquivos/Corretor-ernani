# ============================================
# CONFIGURAÇÕES DE PRODUÇÃO (EXEMPLO)
# ============================================
# ESTE ARQUIVO DEVE SER PREENCHIDO COM SEUS VALORES REAIS ANTES DO DEPLOY!

# ============================================
# BANCO DE DADOS MYSQL
# ============================================
# Senhas fortes e únicas são essenciais para produção
MYSQL_ROOT_PASSWORD=sua_senha_root_muito_forte
MYSQL_DATABASE=corretordasmansoes
MYSQL_USER=corretor_prod
MYSQL_PASSWORD=sua_senha_mysql_muito_forte
MYSQL_PORT=3306

# ============================================
# APLICAÇÃO
# ============================================
NODE_ENV=production
PORT=3000
APP_PORT=3000

# ============================================
# AUTENTICAÇÃO JWT
# ============================================
# Gere um secret forte com: openssl rand -base64 32
JWT_SECRET=SEGREDO_JWT_GERADO_COM_OPENSSL_AQUI

# ============================================
# OAUTH MANUS
# ============================================
VITE_APP_ID=seu_app_id_manus
OAUTH_SERVER_URL=https://api.manus.im
VITE_OAUTH_PORTAL_URL=https://portal.manus.im

# ============================================
# INFORMAÇÕES DO PROPRIETÁRIO
# ============================================
OWNER_OPEN_ID=seu_owner_open_id_manus
OWNER_NAME=Ernani Nunes

# ============================================
# MANUS FORGE API (BACKEND) - Para Agentes de IA
# ============================================
BUILT_IN_FORGE_API_URL=https://forge-api.manus.im
BUILT_IN_FORGE_API_KEY=sua_chave_api_forge_backend

# ============================================
# MANUS FORGE API (FRONTEND) - Para Agentes de IA
# ============================================
VITE_FRONTEND_FORGE_API_URL=https://forge-api.manus.im
VITE_FRONTEND_FORGE_API_KEY=sua_chave_api_forge_frontend

# ============================================
# ANALYTICS
# ============================================
VITE_ANALYTICS_ENDPOINT=https://analytics.manus.im
VITE_ANALYTICS_WEBSITE_ID=seu_id_analytics

# ============================================
# CONFIGURAÇÕES ADICIONAIS
# ============================================
TZ=America/Sao_Paulo
LOG_LEVEL=info
CORS_ORIGINS=https://seu-dominio.com,https://www.seu-dominio.com

# ============================================
# CONFIGURAÇÕES S3 (PARA UPLOAD DE IMAGENS)
# ============================================
AWS_ACCESS_KEY_ID=sua_chave_aws_aqui
AWS_SECRET_ACCESS_KEY=seu_secret_aws_aqui
AWS_REGION=us-east-1
AWS_S3_BUCKET=seu-bucket-s3-aqui
